<program name=' "Notre splendide programme" '/>
    <variable name='toto' value='0' type='integer'/>

    <while condition='toto < 1000'/>

        <variable name='toto' value='toto + 1' type='integer'/>

        <if condition='toto != 300' />
            <variable name='toto' value='toto + 300' type='integer'/>
        </if>
        <else/>
            <variable name='tutu' value='toto + 200' type='integer'/>
        </else>

        <variable name='tutu' value='toto + 200' type='integer'/>

        <for iterator='toto' condition='tutu < 200' step='2' />
            <variable name='tutu' value='tutu + 100' type='integer'/>
        </for>
    </while>

    <variable name='tutu' value='200' type='integer'/>


</program>